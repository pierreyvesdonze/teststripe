{% extends 'base.html.twig' %}

{% block title %}M'enregistrer{% endblock %}

{% block body %}

<section class="register-form">

    <h1>M'enregistrer</h1>
    
    {{ form_start(registrationForm) }}
    {{ form_row(registrationForm.lastname) }}
    {{ form_row(registrationForm.firstname) }}
    {{ form_row(registrationForm.email) }}
    {{ form_row(registrationForm.plainPassword) }}
    {{ form_row(registrationForm.phoneNumber) }}
    {{ form_row(registrationForm.addressFirstLine) }}
    {{ form_row(registrationForm.addressSecondLine) }}
    {{ form_row(registrationForm.addressPostal) }}
    {{ form_row(registrationForm.town) }}
    {{ form_row(registrationForm.submit) }}
    {{ form_end(registrationForm) }}
</section>

{% endblock %}